{
  "name": "ericksonreyes/domain-driven-design",
  "description": "Domain Driven Design Inspired Classes and Interfaces",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=8.0",
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpspec/phpspec": "7.0.*",
    "friends-of-phpspec/phpspec-code-coverage": "6.1.*",
    "phpunit/phpunit": "9.5.*",
    "sebastian/phpcpd": "6.0.*",
    "phing/phing": "2.16.*",
    "mockery/mockery": "1.4.*",
    "php-coveralls/php-coveralls": "2.4.*",
    "php-parallel-lint/php-var-dump-check": "^0.5.0",
    "php-parallel-lint/php-console-color": "v1.0",
    "php-parallel-lint/php-parallel-lint": "1.3.*",
    "phpmd/phpmd": "2.10.*",
    "povils/phpmnd": "2.4.*",
    "squizlabs/php_codesniffer": "3.6.*"
  },
  "autoload": {
    "psr-4": {
      "EricksonReyes\\": "src",
      "spec\\": "spec"
    },
    "psr-0": {
      "": [
        "src"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "bin-dir": "bin"
  }
}
